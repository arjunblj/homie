{
  "name": "homie-ai",
  "version": "0.0.0",
  "description": "Open-source runtime for AI friends.",
  "license": "MIT",
  "type": "module",
  "bin": {
    "homie": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && bun build ./src/index.ts --outdir dist --target bun --format esm --sourcemap && bun build ./src/cli.ts --outdir dist --target bun --format esm --sourcemap",
    "typecheck": "tsc -p tsconfig.json",
    "test": "bun test --coverage"
  },
  "engines": {
    "bun": ">=1.3.0",
    "node": ">=18"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.44",
    "@ai-sdk/openai-compatible": "^2.0.30",
    "@clack/prompts": "^1.0.1",
    "@inkjs/ui": "^2.0.0",
    "ai": "^6.0.86",
    "cli-cursor": "^5.0.0",
    "gradient-string": "^3.0.0",
    "grammy": "^1.40.0",
    "homie-interview-core": "workspace:*",
    "ink": "^6.7.0",
    "marked": "^15.0.0",
    "marked-terminal": "^7.0.0",
    "mppx": "^0.3.4",
    "picocolors": "^1.1.1",
    "qrcode-terminal": "^0.12.0",
    "react": "^19.0.0",
    "smol-toml": "^1.6.0",
    "sqlite-vec": "0.1.7-alpha.2",
    "terminal-link": "^4.0.0",
    "viem": "^2.46.2",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^19.0.0"
  }
}
