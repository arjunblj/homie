{
  "name": "homie-interview-core",
  "version": "0.0.0",
  "description": "Shared interview orchestration for homie.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "bun": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && bun build ./src/index.ts --outdir dist --target bun --format esm --sourcemap",
    "typecheck": "tsc -p tsconfig.json",
    "test": "bun test --coverage"
  },
  "engines": {
    "bun": ">=1.3.0",
    "node": ">=18"
  },
  "dependencies": {
    "zod": "^4.3.6"
  }
}
